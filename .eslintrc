{
    "env": {
        "jest": true,
        "browser": true
    },
    "extends": [
        "airbnb",
        "plugin:jest/recommended",
        "jest-enzyme"
    ],
    "plugins": [
        "babel",
        "import",
        "jsx-a11y",
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true}]
    }
}
